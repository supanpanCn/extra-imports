{
  "name": "extract-modules",
  "version": "0.0.0",
  "description": "Replace the comment with an equal amount of space from the code string",
  "packageManager": "pnpm@7.1.7",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/*.ts --format cjs,esm --dts --splitting --clean",
    "dev": "tsup src/*.ts --format cjs,esm --dts --splitting --clean --watch src"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">= 14"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supanpanCn/extract-modules.git"
  },
  "author": "supanpan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supanpanCn/extract-modules/issues"
  },
  "homepage": "https://github.com/supanpanCn/extract-modules#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "displace-comments": "^0.0.5"
  }
}
